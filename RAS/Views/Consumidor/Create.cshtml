@model Consumidor

<form asp-controller="Consumidor" asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
        <span asp-validation-for="Nome"></span>
    </div>

    <div class="form-group">
        <label>Cashback</label>
        <input asp-for="Cashback" class="form-control" />
        <span asp-validation-for="Cashback"></span>
    </div>

    <div class="form-group">
        <label asp-for="Faixa_etaria"></label>
        <input asp-for="Faixa_etaria" class="form-control" />
        <span asp-validation-for="Faixa_etaria"></span>
    </div>

    <button class="btn btn-primary">Salvar</button>
</form>